function validasi_check(id){
    var payment = document.getElementsByName('payment'+id);
    var i;
    for (i = 0; i < payment.length; i++) {
        if (payment[i].checked) {
            document.getElementById("btn-process-"+id).disabled = false;
        // txt = txt + payment[i].value + " ";
        }
    }
}
